{
  "name": "qst-tracking-lib-example",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "dev:local": "vite --mode local",
    "build": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src/",
    "lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src/",
    "lint:style": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:style:check": "stylelint ./src/**/*.{vue,css,scss}",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest --update",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@itshixun/qst-ui-system": "^0.1.9",
    "@vueuse/core": "^10.3.0",
    "@vueuse/head": "^1.3.1",
    "axios": "^1.4.0",
    "element-plus": "^2.3.9",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.6",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@iconify-json/line-md": "^1.1.27",
    "@iconify-json/mdi": "^1.1.54",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitest/coverage-v8": "^0.34.2",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.0",
    "postcss": "^8.4.28",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.7",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.10.3",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-scss": "^5.1.0",
    "typescript": "^5.1.6",
    "unocss": "^0.55.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vitest": "^0.34.2",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.8"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,json}": "eslint --ignore-path .gitignore --fix",
    "*.{vue,js,ts,jsx,tsx,md,json,scss,css,html}": "prettier . --write",
    "*.{scss,css,vue}": "stylelint --fix"
  }
}
